{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "dataId": {
        "type": "string",
        "minLength": 1,
        "maxLength": 32,
        "examples": [
          "1",
          "1960472"
        ]
      },
      "serialNumber": {
        "type": "string",
        "minLength": 1,
        "maxLength": 50,
        "pattern": "^[a-zA-Z0-9-_]{1,50}$",
        "examples": [
          "00000000000B1",
          "35A058060495422C7934"
        ]
      },
      "latitude": {
        "type": "number",
        "minimum": -90.0,
        "maximum": 90.0,
        "multipleOf": 1E-10,
        "examples": [
          52.0375868826,
          52.172644
        ]
      },
      "longitude": {
        "type": "number",
        "minimum": -180.0,
        "maximum": 180.0,
        "multipleOf": 1E-10,
        "examples": [
          21.1956136,
          20.026094
        ]
      },
      "altitude": {
        "type": [
          "number",
          "null"
        ],
        "minimum": -1000.0,
        "maximum": 4000.0,
        "multipleOf": 0.01,
        "examples": [
          10.0,
          200.02
        ]
      },
      "altitude": {
        "type": [
          "number",
          "null"
        ],
        "minimum": -1000.0,
        "maximum": 4000.0,
        "multipleOf": 0.01,
        "examples": [
          10.0,
          200.02
        ]
      },
      "fixTimeEpoch": {
        "type": "integer",
        "minimum": 1505869200000
      },
      "gpsSpeed": {
        "type": "number",
        "minimum": 0.0,
        "maximum": 56.0,
        "multipleOf": 0.01,
        "examples": [
          3.21,
          20.0
        ]
      },
      "accuracy": {
        "type": "number",
        "minimum": 0.0,
        "multipleOf": 0.01,
        "examples": [
          10.14,
          30.0
        ]
      },
      "gpsHeading": {
        "type": "number",
        "minimum": 0.0,
        "maximum": 360.0,
        "multipleOf": 0.01,
        "examples": [
          40.14,
          230.0
        ]
      },
      "eventType": {
        "type": "string",
        "enum": [
          "turnon",
          "turnoff",
          "startjourney",
          "endjourney",
          "plugout",
          "plugon",
          "gsmonline",
          "gsmoffline",
          "gpsonline",
          "gpsoffline",
          "jamming",
          "spoofing",
          "location"
        ]
      },
      "lac": {
        "type": "string",
        "pattern": "^[A-Fa-f0-9]{4}$"
      },
      "mcc": {
        "type": "string",
        "pattern": "^[0-9]{3}$"
      },
      "mnc": {
        "type": "string",
        "pattern": "^[0-9]{2,3}$"
      },
      "mobileCellId": {
        "type": "string",
        "pattern": "^[A-Fa-f0-9]{9}$"
      },
      "satellitesForFix": {
        "type": "integer",
        "maximum": 90,
        "minimum": 0
      },
      "satellitesInView": {
        "type": "integer",
        "maximum": 90,
        "minimum": 0
      }
    },
    "required": [
      "dataId",
      "serialNumber",
      "latitude",
      "longitude",
      "fixTimeEpoch",
      "gpsSpeed",
      "gpsHeading",
      "eventType",
      "satellitesForFix"
    ]
  }
}